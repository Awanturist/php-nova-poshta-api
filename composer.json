{
  "name": "bladl/nova-poshta-api",
  "description": "PHP implementation of Ukrainian delivery service Nova Poshta API 2.0",
  "type": "library",
  "license": "MIT",
  "version": "1.0.5",
  "authors": [
    {
      "name": "BladL"
    }
  ],
  "keywords": [
    "Нова Пошта",
    "API 2.0"
  ],
  "autoload": {
    "psr-4": {
      "BladL\\NovaPoshta\\": "src"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm --output-format=phpstorm",
    "php-cs": "vendor/bin/php-cs-fixer fix",
    "test": [
      "@psalm"
    ],
    "fix": [
      "@php-cs"
    ]
  },
  "prefer-stable": true,
  "minimum-stability": "stable",
  "require-dev": {
    "friendsofphp/php-cs-fixer": ">=3.6",
    "vimeo/psalm": ">=4.6"
  },
  "config": {
    "allow-plugins": {
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "bladl/time": "^0.0.1"
  }
}
